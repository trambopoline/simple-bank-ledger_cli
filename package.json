{
	"name": "simple-bank-ledger_front",
	"version": "0.0.1",
	"description": "The front end of a simple bank ledger.",
	"main": "index.js",
	"scripts": {
		"build": "babel src --out-dir dist",
		"start": "VERSION=$npm_package_version DESCRIPTION=$npm_package_description node dist/index.js",
		"watch": "npm-watch build"
	},
	"watch": {
		"build": {
			"patterns": [
				"src"
			]
		}
	},
	"keywords": [
		"simple",
		"bank",
		"cli"
	],
	"author": "Devin Dodson",
	"license": "MIT",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"npm-watch": "^0.4.0"
	},
	"dependencies": {
		"chalk": "^2.4.1",
		"commander": "^2.19.0",
		"lodash": "^4.17.11",
		"node-cache": "^4.2.0",
		"restify-clients": "^2.6.2"
	}
}
